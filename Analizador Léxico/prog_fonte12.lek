FLUT numero //Declaracao de Variavel.

PRINTA("DIGITE UM NUMERO: ") //.
PEGA(numero) //.

TESTE
{
	(numero <= 10){  // primeira condicao.
		PRINTA("Entrada eh menor ou igual a 10") //.
	} 
	(numero == 11){  // segunda condicao.
		PRINTA("Entrada eh igual a 11") //.
	}
	(numero == 12){  // terceira condicao.
		PRINTA("Entrada eh igual a 12") //.
	} 
	OUTROCASO {  // quarta condicao.
		PRINTA("Entrada eh maior ou igual a 13") //.
	} 
	
}

