STRING nome, teste //Declaracao de Variavel.

PRINTA("DIGITE UM NOME: ")//.
PEGA(nome)//.

SE(nome == "sair")
{//.
	PRINTA("TEM CERTEZA DISSO (sim ou nao)?")//.
	PEGA(teste)//.
	SE(teste == "sim")
	{//.
		PRINTA("SAIU")//.
	}
	SENAO
	{//.
		PRINTA("NAO SAIU")//.
	}
}
SENAO
{//.
	PRINTA("NAO SAIU")//.
}

