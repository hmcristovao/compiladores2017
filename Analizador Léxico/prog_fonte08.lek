BOOL  flag //Declaracao de variavel logica.

PRINTA("DIGITE UM VALOR LOGICO (1 OU 0): ") // solicita um valor logico.
PEGA(flag) // entrada de um valor logico (0 ou 1).

SE(flag == true)
{ // se o valor digitado for verdadeiro.
    PRINTA("Verdadeiro") //.
}
SENAO
{ // se o valor digitado for falso.
    PRINTA("Falso") //.
}