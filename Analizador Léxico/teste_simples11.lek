FLUT numero //Declaracao de Variavel.

PRINTA("DIGITE UM NUMERO DE 1 A 3: ") //.
PEGA(numero) //.

TESTE
{
	(numero == 1){  // primeira condicao.
		PRINTA("Numero UM digitado") //.
	} 
	(numero == 2){  // segunda condicao.
		PRINTA("Numero DOIS digitado") //.
	}
	(numero == 3){  // terceira condicao.
		PRINTA("Numero TRES digitado") //.
	} 
	OUTROCASO {  // quarta condicao.
		PRINTA("Entrada incorreta!") //.
	} 
}
